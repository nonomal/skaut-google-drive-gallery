{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "script"
	},
	"env": {
		"browser": true,
		"node": false
	},
	"plugins": [
		"compat",
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@wordpress/eslint-plugin/recommended",
		"plugin:@typescript-eslint/eslint-recommended"
	],
	"rules": {
		"compat/compat": "warn"
	},
	"globals": {
		"SgdgBlockIconComponent": false,
		"SgdgBooleanSettingsComponent": false,
		"SgdgEditorComponent": false,
		"SgdgIntegerSettingsComponent": false,
		"SgdgOrderingSettingsComponent": false,
		"SgdgSettingsComponent": false,
		"SgdgSettingsOverrideComponent": false,
		"jQuery": false,
		"sgdgBlockLocalize": false,
		"sgdgRootpathLocalize": false,
		"sgdgShortcodeLocalize": false,
		"sgdgTinymceLocalize": false,
		"tb_remove": false,
		"tb_show": false
	}
}
